<Window x:Class="Nokia.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nokia"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="_Username" HorizontalAlignment="Left" Margin="348,167,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="_Password" HorizontalAlignment="Left" Margin="344,200,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="_Login" Content="Login" HorizontalAlignment="Center" Margin="0,239,0,0" VerticalAlignment="Top" Click="_Login_Click"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="-55,-26,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
