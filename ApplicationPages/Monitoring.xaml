<Window x:Class="Nokia.ApplicationPages.Monitoring"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nokia.ApplicationPages"
        mc:Ignorable="d"
        Title="Monitoring" Height="450" Width="800">
    <Grid Background="{StaticResource BackgroundGradient}">
        <Button Style="{StaticResource BackButton}"/>
        <TextBlock Text="This is the monitoring page!" Margin="0,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Button x:Name="_Analyze" Content="Analyze" FontSize="16" HorizontalAlignment="Center" Margin="0,364,0,0" VerticalAlignment="Top" Height="30" Width="200"/>
    </Grid>
</Window>
